{
  "family": "simple-docker-service-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::808677050841:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "simple-docker-container",
      "image": "808677050841.dkr.ecr.ap-south-1.amazonaws.com/simple-docker-service:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
